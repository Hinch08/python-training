str1=input('enter a random string1:')
str2=input('enter a random string2:')

length1=len(str1)
length2=len(str2)

if length1>length2:
    print('str1 is longer')
elif length2>length1:
    print('str2 is longer')
else:
    print('They are of equal lengths')

  